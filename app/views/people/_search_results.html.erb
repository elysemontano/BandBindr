 <% @people.each do |person| %>
    <button>
    <%= link_to organization_person_path(@current_organization, person) do %>
    <tr onclick="location.href='<%= url_for(organization_person_path(@current_organization, person)) %>'" class="bg-white border-b dark:bg-gray-800 dark:border-gray-700 hover:bg-gray-50 dark:hover:bg-gray-600">
        <td class="px-6 py-4 font-medium text-gray-900 whitespace-nowrap dark:text-white">
        <%= person.first_name %>
        </td>
        <td class="px-6 py-4 font-medium text-gray-900 whitespace-nowrap dark:text-white">
        <%= person.last_name %>
        </td>
    </tr>
    <% end %>
    </button>
<% end %>